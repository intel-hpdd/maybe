{
  "name": "intel-maybe",
  "version": "2.1.0",
  "description": "The Maybe datatype",
  "main": "dist/source/maybe.js",
  "scripts": {
    "test:dev": "npm run prepublish && npm t",
    "test": "NODE_ENV=test ./bin/test.js",
    "eslint": "eslint ./",
    "flow": "flow",
    "prepublish": "./bin/build.sh",
    "cover:dev": "npm run cover -- --reporter=html",
    "cover": "NODE_ENV=test ./bin/build.sh cover && nyc --reporter=text npm t"
  },
  "nyc": {
    "sourceMap": false,
    "instrument": false
  },
  "publishConfig": {
    "registry": "https://ubit-artifactory-or.intel.com/artifactory/api/npm/iml-npm-local"
  },
  "pre-commit": [
    "eslint",
    "flow",
    "test"
  ],
  "repository": {
    "type": "git",
    "url": "git@github.intel.com:IML/maybe.git"
  },
  "author": "IML Team",
  "license": "MIT",
  "dependencies": {
    "intel-fp": "7.0.0"
  },
  "devDependencies": {
    "babel-cli": "6.18.0",
    "babel-core": "6.18.2",
    "babel-eslint": "7.1.0",
    "babel-plugin-__coverage__": "11.0.0",
    "babel-plugin-transform-class-properties": "6.18.0",
    "babel-plugin-transform-flow-strip-types": "6.18.0",
    "babel-plugin-transform-strict-mode": "6.18.0",
    "babel-preset-es2015": "6.18.0",
    "eslint": "3.9.1",
    "eslint-plugin-flowtype": "2.25.0",
    "eslint-plugin-fp": "2.2.0",
    "eslint-plugin-import": "2.1.0",
    "flow-bin": "0.34.0",
    "intel-flow-jasmine": "1.5.0",
    "intel-jasmine-junit-reporter": "1.0.0",
    "intel-jasmine-n-matchers": "2.1.1",
    "jasmine": "2.5.2",
    "nyc": "8.4.0",
    "pre-commit": "1.1.3"
  }
}
